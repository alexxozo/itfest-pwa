{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "serviceworker": true
    },
    "globals": {
        "workbox": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "extends": "eslint:recommended",
    "rules": {
